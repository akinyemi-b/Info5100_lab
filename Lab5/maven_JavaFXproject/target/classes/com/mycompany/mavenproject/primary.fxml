<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="482.0" prefWidth="590.0" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.mavenproject.PrimaryController">
   <children>
      <AnchorPane prefHeight="502.0" prefWidth="550.0">
         <children>
            <Label fx:id="titleLabel" layoutX="230.0" layoutY="59.0" text="Registration">
               <font>
                  <Font size="20.0" />
               </font>
            </Label>
            <Label fx:id="nameLabel" layoutX="187.0" layoutY="138.0" text="Name:" />
            <TextField fx:id="nameTextField" layoutX="284.0" layoutY="133.0" />
            <Button fx:id="submitButton" layoutX="275.0" layoutY="351.0" mnemonicParsing="false" onAction="#submit" text="Submit" />
            <Label fx:id="ageLabel" layoutX="187.0" layoutY="200.0" text="Age:" />
            <TextField fx:id="ageTextField" layoutX="286.0" layoutY="185.0" prefHeight="31.0" prefWidth="187.0" />
            <TextField fx:id="emailTextField" layoutX="286.0" layoutY="254.0" />
            <Label fx:id="emailLabel" layoutX="187.0" layoutY="259.0" text="Email:" />
         </children>
      </AnchorPane>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
